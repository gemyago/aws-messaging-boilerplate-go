{
  "defaultLogLevel": "INFO",
  "gracefulShutdownTimeout": "10s",
  "httpServer": {
    "port": 8080,
    "idleTimeout": "60s",
    "readHeaderTimeout": "2s",
    "readTimeout": "2s",
    "writeTimeout": "8s"
  },
  "aws": {
    "region": "us-east-1",
    "sns": {
      "messagesTopicArn": "set via env var: APP_SNS_MESSAGES_TOPIC_ARN"
    },
    "sqs": {
      "maxPollWaitTimeSec": 20,
      "messagesQueueUrl": "set via env var: APP_SQS_MESSAGES_QUEUE_URL",
      "messagesQueueVisibilityTimeoutSec": 10
    }
  }
}